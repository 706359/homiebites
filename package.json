{
  "name": "homiebites",
  "version": "1.0.0",
  "description": "HomieBites - Premium Tiffin Service",
  "type": "module",
  "scripts": {
    "dev": "cd web && npm run dev",
    "build": "npm run web:build",
    "check:buttons": "node scripts/check-buttons.cjs",
    "web": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "web:start": "cd web && npm start",
    "admin": "cd admin && npm run dev",
    "admin:build": "cd admin && npm run build",
    "admin:preview": "cd admin && npm run preview",
    "backend": "cd backend && npm start",
    "dev:full": "concurrently \"npm run web\" \"npm run backend\"",
    "dev:all": "concurrently \"npm run web\" \"npm run admin\" \"npm run backend\"",
    "push": "git add . && git commit -m 'updated' && git push"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.20.2",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2",
    "vite": "^5.0.8",
    "xlsx": "^0.18.5"
  },
  "author": "",
  "license": "ISC"
}
