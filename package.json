{
  "name": "homiebites",
  "version": "1.0.0",
  "description": "HomieBites - Premium Tiffin Service",
  "type": "module",
  "scripts": {
    "dev": "cd web-admin && npm run dev",
    "build": "npm run web:build",
    "check:buttons": "node scripts/check-buttons.cjs",
    "web": "cd web-admin && npm run dev",
    "web:build": "cd web-admin && npm run build",
    "web:start": "cd web-admin && npm start",
    "admin": "cd web-admin && npm run dev",
    "admin:build": "cd web-admin && npm run build",
    "admin:start": "cd web-admin && npm start",
    "backend": "cd backend && npm start",
    "dev:full": "cd web-admin && npm run dev",
    "dev:all": "cd web-admin && npm run dev",
    "stop": "lsof -ti:3000,3001,3002 | xargs kill -9 2>/dev/null || echo 'No processes found on ports 3000, 3001, 3002'",
    "stop:web": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'No process found on port 3000'",
    "stop:admin": "lsof -ti:3002 | xargs kill -9 2>/dev/null || echo 'No process found on port 3002'",
    "stop:backend": "lsof -ti:3001 | xargs kill -9 2>/dev/null || echo 'No process found on port 3001'",
    "check:dead": "node scripts/check-dead-files.js",
    "scan:unused": "node scripts/scan-unused-files.js",
    "verify:safe": "node scripts/verify-safe-to-delete.js",
    "cleanup:safe": "node scripts/safe-cleanup.js",
    "cleanup:safe:dry": "node scripts/safe-cleanup.js --dry-run",
    "push": "git add . && git commit -m 'updated' && git push"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.20.2",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2",
    "vite": "^5.0.8",
    "xlsx": "^0.18.5"
  },
  "author": "",
  "license": "ISC"
}
